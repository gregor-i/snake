{
  "devDependencies": {
    "http-server": "^0.12.3",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "develop": "npm run mkdir && run-p server watch",
    "mkdir": "mkdir -p build",
    "server": "http-server -c-1 build",
    "build": "run-p build-*",
    "build-html": "cp src/main/html/* build/.",
    "watch": "run-p watch-*",
    "watch-html": "nodemon --exec 'npm run build-html' --watch frontend/src/main/html -e html"
  },
  "dependencies": {  }
}
